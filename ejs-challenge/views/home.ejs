<%- include('partials/header'); -%>

<h1>Home</h1>
<p><%= homeContent %></p>

<% posts.forEach(function(post) { %>
      <h1><%= post.postTitle %></h1>
      <p><%= post.postBody.substring(0, 100) + " ..." %> <a href="/posts/<%= post.postId %>">Read More</a></p>
<% }); %>
<%- include('partials/footer'); -%>
