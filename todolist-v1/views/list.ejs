<%- include('header'); -%>

    <div class="box" id="heading">
      <h1><%= kindOfDay %></h1>
    </div>

    <div class="box">
      <% newListItems.forEach(function(item){ %>
        <div class="item">
          <input type="checkbox">
          <p> <%= item.name %> </p>
        </div>
      <% }) %>

      <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="Add a task" autocomplete="off">
        <button type="submit" name="addItem">+</button>
      </form>
    </div>


<%- include('footer'); -%>
